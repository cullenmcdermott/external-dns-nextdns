apiVersion: v1
kind: Secret
metadata:
  name: nextdns-webhook-secret
  namespace: external-dns
type: Opaque
stringData:
  # Replace with your actual NextDNS API key
  NEXTDNS_API_KEY: "your-nextdns-api-key-here"
  # Replace with your actual NextDNS Profile ID
  NEXTDNS_PROFILE_ID: "your-profile-id-here"
